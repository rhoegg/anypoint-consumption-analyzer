# Anypoint Platform Connected App Credentials
# Create a Connected App in Anypoint Platform with the required permissions
ANYPOINT_CLIENT_ID=your-client-id-here
ANYPOINT_CLIENT_SECRET=your-client-secret-here

# Configuration Options

# Enable debug mode
DEBUG=false

# Generate CSV reports
EXPORT_CSV=true

# Attempt to download JAR files
# Note: JAR downloads may fail due to API limitations
DOWNLOAD_JARS=true

# Number of days of monitoring data to analyze
# Longer periods provide more accurate message volume estimates
ANALYZE_DAYS=30
